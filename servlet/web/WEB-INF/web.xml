<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>myServlet</servlet-name>
        <servlet-class>com.oracle.servlet.MyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>myServlet</servlet-name>
        <url-pattern>/index</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>myDelete</servlet-name>
        <servlet-class>com.oracle.servlet.myServletDelete</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>myDelete</servlet-name>
        <url-pattern>/delete</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>myUser</servlet-name>
        <servlet-class>com.oracle.servlet.UserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>myUser</servlet-name>
        <url-pattern>/myUser</url-pattern>
    </servlet-mapping>
</web-app>